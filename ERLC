if not getgenv().ScriptAlreadyExecuted then
    game:GetService("StarterGui"):SetCore("SendNotification", {
        Title = "Mineblox - Error",
        Text = "You must execute this script through the main loader!",
        Duration = 5
    })
    task.wait(5)
    game:GetService("Players").LocalPlayer:Kick("HWID & Loader enforcement.")
    return
end

loadstring(game:HttpGet("https://pastebin.com/raw/MbMKAMaN"))()
