if getgenv().ScriptAlreadyExecuted then
			game:GetService("StarterGui"):SetCore("SendNotification", {
				Title = "Mineblox - Error";
				Text = "You have already executed this script!",
				Duration = 3,
			})
	return
end
getgenv().ScriptAlreadyExecuted = true

local Players = game:GetService("Players")
local player = Players.LocalPlayer

local GameLoaders = {
	[903807016] = function()
		loadstring(game:HttpGet("https://raw.githubusercontent.com/TheBypasserOnTop/MinebloxLoader/refs/heads/main/ERLC"))()
	end,
}

local GameId = game.GameId

local loader = GameLoaders[GameId]
if not loader then
	player:Kick("This game isn't supported, sorry!")
	return
end

loader()
